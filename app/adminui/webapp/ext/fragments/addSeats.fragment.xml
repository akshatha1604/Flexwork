<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core" core:require="{ext : 'adminui/ext/controller/ObjectPageExt'}">
        <Dialog title="{NewModel>/Property/title}">
        <content>
            <VBox width="550px">
                <HBox>
                    <!--<Text text='Group booking/ my booking/ booking of behalf'/>-->
                    <RadioButtonGroup buttons="{NewModel>/radiobutton}" selectedIndex="{NewModel>/Property/SelectedIndex}"
                     select="ext.onCustomValueSelected">
                        <buttons>
                            <RadioButton text="{NewModel>Seat}"/>
                        </buttons>
                    </RadioButtonGroup>
                </HBox>
                <Input placeholder="{NewModel>/Property/placeHolder}" value="{NewModel>/Property/inputField}"></Input>
            </VBox>
        </content>
        <buttons>
            <Button text="Save" type="Default" iconFirst="true" enabled="true" visible="true" iconDensityAware="false" press="ext.SaveSeatPress"/>
            <Button text="Cancel" type="Default" iconFirst="true" enabled="true" visible="true" iconDensityAware="false" press="ext.AddSeatCancelPress"/>        
        </buttons>
    </Dialog>
</core:FragmentDefinition>