<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified" core:require="{ext : 'adminui/ext/controller/ObjectPageExt'}">
    <Dialog title='{NewImageModel>/popupTitle}'>
        <content>
            <VBox width="430px">
                <HBox>
                    <l:VerticalLayout>
                        <u:FileUploader id="fileUploader" name="myFileUpload"  uploadUrl="upload/" tooltip="Upload your file to the local server" uploadComplete="ext.handleUploadComplete"/>
                        <Button text="Upload File" press="ext.handleUploadPress"/>
                    </l:VerticalLayout>
                </HBox>
                <!--<Input placeholder="enter seat no. in mentioned format" value="{NewModel>/Property/inputField}"></Input>-->
            </VBox>
        </content>
        <buttons>
            <!-- <Button text="Save" type="Default" iconFirst="true" enabled="true" visible="true" iconDensityAware="false" press="DialogSavePress"/> -->
            <Button text="Cancel" type="Default" iconFirst="true" enabled="true" visible="true" iconDensityAware="false" press="ext.closeImageUploadDial"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>